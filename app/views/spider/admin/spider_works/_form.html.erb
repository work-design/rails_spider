<%= form_with model: [:admin, @spider_work], local: true do |f| %>
  <%= render 'shared/error_messages', target: @spider_work %>
  <%= f.text_field :name %>
  <%= f.text_field :host %>
  <%= f.text_field :list_path %>
  <%= f.text_field :item_path %>
  <%= f.text_field :page_params %>
  <%= f.fields :spider_resources, on: { wrapper_all: false, label: false, placeholder: true }, css: { wrapper_input: 'two wide field' } do |ef| %>
    <%= render partial: 'item_form', locals: { ef: ef } %>
  <% end %>
  <%= f.submit %>
<% end %>
